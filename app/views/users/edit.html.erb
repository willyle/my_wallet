<%= form_for @user, url: "/profile/update" do |f| %>
	<%= f.label :first_name, "First Name" %>
	<%= f.text_field :first_name %><br>
	<%= f.label :last_name, "Last Name" %>
	<%= f.text_field :last_name %><br>
	<%= f.label :email, "Email" %>
	<%= f.email_field :email %><br>
	<%= f.label :phone, "Phone" %>
	<%= f.telephone_field :phone %><br>
	<%= f.submit "Update" %>
<% end %>